label.recaptcha_input_area_text {
	text-transform: none !important;
}
#recaptcha_response_field {
	box-shadow: 0;
	-o-box-shadow: 0;
	-moz-box-shadow: 0;
	-webkit-box-shadow: 0;

	-moz-border-radius: 0;
	-webkit-border-radius: 0;
	border-radius: 0;

	min-width: 20px;
}

