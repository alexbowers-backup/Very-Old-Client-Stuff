@font-face {
	font-family: 'CopseRegular';
	src: url('Copse-Regular-webfont.eot');
	src: local('â˜º'), url('type/Copse-Regular-webfont.woff') format('woff'), url('type/Copse-Regular-webfont.ttf') format('truetype'), url('type/Copse-Regular-webfont.svg#webfontWKqJmDgH') format('svg');
	font-weight: normal;
	font-style: normal;
}
body {
	background: #8C8C8C;
	font: 11pt/15.6pt Frutiger,"Frutiger Linotype",Univers,Calibri,"Gill Sans","Gill Sans MT","Myriad Pro",Myriad,"DejaVu Sans Condensed","Liberation Sans","Nimbus Sans L",Tahoma,Geneva,"Helvetica Neue",Helvetica,Arial,"sans serif";
}
a {
	text-decoration: none;
	border-bottom: 1px dotted;
	color: #991B12;
}
a.i, a img {
	border: 0 none;
}
a:hover {
	color: #BC2519;
}
a:active {
	position: relative;
	top: 1px;
}
#hold {
	min-width: 300px;
	max-width: 550px;
	margin: auto;
}
/*#container, header { 
	width: 550px;
	margin: auto;
}*/
#container {
	background: #DADADA;
	border: 1px solid white;
	border-top: 0 none;
	padding: 20px 30px;
	
	-moz-border-radius: 0 0 5px 5px;
	-webkit-border-radius: 0 0 5px 5px;
	border-radius: 0 0 5px 5px;
	
	box-shadow: 0 2px 0 rgba(0,0,0,0.1);
	-o-box-shadow: 0 2px 0 rgba(0,0,0,0.1);
	-moz-box-shadow: 0 2px 0 rgba(0,0,0,0.1);
	-webkit-box-shadow: 0 2px 0 rgba(0,0,0,0.1);
}
.col {
	/*width: 45%;
	float: left;*/
}
.clearfix:after {
    content: ".";
    display: block;
    height: 0;
    clear: both;
    visibility: hidden;
}
header {
	display: block;
	margin-top: 80px;
	text-align: center;
	padding: 0 30px;
	
	height: 100px;
	color: #F0D67D;
	background: url('img/head_bg.png') #901810;
	border: 1px solid #901810;
	
	-moz-border-radius: 5px 5px 0 0;
	-webkit-border-radius: 5px 5px 0 0;
	border-radius: 5px 5px 0 0;
}
footer {
	margin: 7px 0;
	color: rgba(0,0,0,0.25);
	text-align: center;
	text-shadow: 0 1px 0 rgba(255,255,255,0.1);
	display: block;
}

/* forms */
legend {
	/*font: 15pt Georgia,"Times New Roman",sans-serif;*/
	font: 15pt 'CopseRegular',Frutiger,"Frutiger Linotype",Univers,Calibri,"Gill Sans","Gill Sans MT","Myriad Pro",Myriad,"DejaVu Sans Condensed","Liberation Sans","Nimbus Sans L",Tahoma,Geneva,"Helvetica Neue",Helvetica,Arial,"sans serif";
	padding: 0;
}
fieldset {
	padding: 0;
	padding-top: 1px;
	border: 0 none;
}
fieldset ol {
	padding: 0;
	margin: 14px 0;
	list-style: none;
}
label {
	display: block;
	color: rgba(0,0,0,0.55);
	font-size: 10pt;
	text-shadow: 0 1px 0 rgba(255,255,255,0.2);
	text-transform: uppercase;
	margin-top: 10px;
}
input[type="text"], input[type="email"], input[type="number"], input[type="url"], input[type="password"] {
	/*min-width: 147px;*/
	min-width: 167px;
	border: 1px solid rgba(0,0,0,0.2);
	background: #F5F5F5;
	padding: 6px 8px;
	
	box-shadow: inset 0 2px 3px rgba(0,0,0,0.05), 0 1px 0 rgba(255,255,255,0.2);
	-o-box-shadow: inset 0 2px 3px rgba(0,0,0,0.05), 0 1px 0 rgba(255,255,255,0.2);
	-moz-box-shadow: inset 0 2px 3px rgba(0,0,0,0.05), 0 1px 0 rgba(255,255,255,0.2);
	-webkit-box-shadow: inset 0 2px 3px rgba(0,0,0,0.05), 0 1px 0 rgba(255,255,255,0.2);
	
	-moz-border-radius: 3px;
	-webkit-border-radius: 3px;
	border-radius: 3px;
}
input[type="button"], input[type="submit"] {
	padding: 4px 11px;
	border: 1px solid rgba(0,0,0,0.2);
	background: #E1E1E1;
	color: rgba(0,0,0,0.8);
	
	box-shadow: inset 0 2px 2px rgba(255,255,255,0.3), inset 0 -2px 2px rgba(0,0,0,0.03), 0 1px 0 rgba(0,0,0,0.02);
	-o-box-shadow: inset 0 2px 2px rgba(255,255,255,0.3), inset 0 -2px 2px rgba(0,0,0,0.03), 0 1px 0 rgba(0,0,0,0.02);
	-moz-box-shadow: inset 0 2px 2px rgba(255,255,255,0.3), inset 0 -2px 2px rgba(0,0,0,0.03), 0 1px 0 rgba(0,0,0,0.02);
	-webkit-box-shadow: inset 0 2px 2px rgba(255,255,255,0.3), inset 0 -2px 2px rgba(0,0,0,0.03), 0 1px 0 rgba(0,0,0,0.02);

	-moz-border-radius: 3px;
	-webkit-border-radius: 3px;
	border-radius: 3px;
}
input[type="button"]:active, input[type="submit"]:active {
	position: relative;
	top: 1px;
}
input[type="button"]:hover, input[type="submit"]:hover {
	background: #E4E4E4;
}

::-webkit-input-placeholder { color: rgba(0,0,0,0.2); }
input:-moz-placeholder { color: rgba(0,0,0,0.2); }

